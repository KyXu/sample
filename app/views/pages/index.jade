extends ../layout
block content
  .container
    .row(style="padding-bottom:50px;")
      .panel.panel-default
        .panel-heading
          h3 Customers list
        .panel-body
        each bp in bps
          .col-md-2
            .caption
              h3 #{bp.code}
              H3 #{bp.name}
                p: a.btn.btn-primary(href="",role="button") View
